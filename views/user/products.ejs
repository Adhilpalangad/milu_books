					<script>
						// Handle Add to Cart with SweetAlert
						document.querySelectorAll('form[id^="add-to-cart-form"]').forEach(form => {
							form.addEventListener('submit', function(event) {
								event.preventDefault(); // Prevent the default form submission to show the alert first
				
								Swal.fire({
									icon: 'success',
									title: 'Added to Cart',
									text: 'The product has been added to your cart!',
									showConfirmButton: false,
									timer: 1500
								}).then(() => {
									form.submit(); // Continue form submission after the alert
								});
							});
						});
					</script>
